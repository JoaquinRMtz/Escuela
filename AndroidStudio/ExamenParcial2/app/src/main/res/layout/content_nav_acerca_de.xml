<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="i0akinsec.wordpress.com.examenparcial2.NavAcercaDeActivity"
    tools:showIn="@layout/activity_nav_acerca_de">

    <TextView
        android:layout_width="368dp"
        android:layout_height="wrap_content"
        android:text="@string/acercade_menu"
        android:layout_marginLeft="@dimen/margen_centro"
        android:textSize="@dimen/size_titulo"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margen_centro"
        android:textSize="@dimen/tam_texto"
        android:text="@string/acercade_info"/>
</LinearLayout>
